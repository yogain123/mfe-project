{
  "name": "shell",
  "version": "1.0.0",
  "description": "Shell/Host application for MFE architecture",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.config.js --mode development",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "start": "webpack serve --config webpack.config.js --mode production --port 3000",
    "deploy:s3": "aws s3 sync dist/ s3://$S3_BUCKET_NAME/mfe/shell/ --delete --cache-control 'no-cache, no-store, must-revalidate, max-age=0' --expires '1970-01-01T00:00:00Z' --acl public-read"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-env": "^7.18.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "keywords": [
    "micro-frontend",
    "shell",
    "host",
    "webpack",
    "module-federation"
  ],
  "author": "MFE Learning Project",
  "license": "MIT"
}
